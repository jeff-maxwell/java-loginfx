<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="292.0" prefWidth="352.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ChangePasswordController">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Change Password">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="34.0" layoutY="67.0" text="Current Password" />
      <PasswordField fx:id="txtCurrentPassword" layoutX="34.0" layoutY="84.0" prefHeight="25.0" prefWidth="289.0" />
      <Label layoutX="34.0" layoutY="121.0" text="New Password" />
      <PasswordField fx:id="txtNewPassword" layoutX="30.0" layoutY="138.0" prefHeight="25.0" prefWidth="289.0" />
      <Label layoutX="34.0" layoutY="174.0" text="Confirm New Password" />
      <PasswordField fx:id="txtConfirmNewPassword" layoutX="30.0" layoutY="200.0" prefHeight="25.0" prefWidth="289.0" />
      <Button fx:id="btnChange" layoutX="70.0" layoutY="239.0" mnemonicParsing="false" onAction="#change" text="Change" />
      <Button fx:id="btnCancel" layoutX="232.0" layoutY="239.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
   </children>
</AnchorPane>
