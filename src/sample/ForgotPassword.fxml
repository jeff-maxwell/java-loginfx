<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="185.0" prefWidth="345.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ForgotPasswordController">
   <children>
      <Label layoutX="40.0" layoutY="14.0" text="Forgot Password">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="btnEmail" layoutX="151.0" layoutY="67.0" mnemonicParsing="false" onAction="#email" text="Email" />
      <Button fx:id="btnCancel" layoutX="151.0" layoutY="138.0" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
      <Button fx:id="btnSecurity" layoutX="122.0" layoutY="100.0" mnemonicParsing="false" onAction="#security" text="Security Question" />
   </children>
</AnchorPane>
