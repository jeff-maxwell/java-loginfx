   <children>
      <Label layoutX="14.0" layoutY="14.0" text="Home">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Button fx:id="btnLogout" layoutX="534.0" layoutY="14.0" mnemonicParsing="false" onAction="#logout" text="Logout" />
      <Button fx:id="btnChangePassword" layoutX="413.0" layoutY="14.0" mnemonicParsing="false" onAction="#changePassword" text="Change Password" />
      <Label fx:id="lblName" layoutX="300.0" layoutY="133.0" text="[Name]" />
      <Label fx:id="lblUsername" layoutX="300.0" layoutY="183.0" text="[Username]" />
      <Label fx:id="lblEmail" layoutX="300.0" layoutY="224.0" text="[Email]" />
   </children>
</AnchorPane>
