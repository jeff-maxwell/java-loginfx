<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="252.0" prefWidth="673.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SecurityController">
   <children>
      <Label layoutX="45.0" layoutY="14.0" text="Forgot Password: Security Questions">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="131.0" layoutY="81.0" text="Security Question" />
      <Label fx:id="lblQuestion" layoutX="465.0" layoutY="81.0" />
      <Label layoutX="131.0" layoutY="135.0" text="Answer" />
      <TextField fx:id="txtAnswer" layoutX="391.0" layoutY="140.0" />
      <Button fx:id="btnSubmit" layoutX="225.0" layoutY="213.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <Button fx:id="btnBack" layoutX="350.0" layoutY="213.0" mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
</AnchorPane>
