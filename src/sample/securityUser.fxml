<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="226.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SecurityUserController">
   <children>
      <Label layoutX="160.0" layoutY="14.0" text="Security Question">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label layoutX="160.0" layoutY="96.0" text="Enter Username" />
      <TextField fx:id="txtUsername" layoutX="291.0" layoutY="92.0" />
      <Button fx:id="txtSubmit" layoutX="244.0" layoutY="179.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
      <Button fx:id="txtBack" layoutX="340.0" layoutY="179.0" mnemonicParsing="false" onAction="#back" text="Back" />
   </children>
</AnchorPane>
